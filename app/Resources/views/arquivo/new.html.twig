{% extends 'base.html.twig' %}

{% block body %}
    <h1>Upload Arquivo ENADE</h1>


    {{ form_start(form, {'attr': {'class' : 'form-horizontal'}}) }}

    <div class="form-group">
        <div class="col-md-12">
            <label class="control-label">{{ form_label(form.arquivoVich) }}</label>
            {{ form_widget(form.arquivoVich) }}
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-12">
            {{ form_widget(form.save, { 'attr': {'class': 'btn btn-sm btn-success','row':'20'}}) }}
            ou
            <a class="btn btn-sm btn-default" href="{{ path('arquivo_index') }}">Lista de arquivos</a>
        </div>
    </div>

    {{ form_end(form) }}
{% endblock %}
